<section>
    <h1>Products</h1>
    <article class="productCard-container">
        {{#each products}}
        <div class="productCard">
            <h3>{{title}}</h3>
            <p>{{description}}</p>
            <p>Price: ${{price}}</p>
            <p>Stock: {{stock}}</p>
            <p>Category: {{category}}</p>
        </div>
        {{/each}}
    </article>
</section>